          //- ! copied in publication.html.jade
          mixin display_publication(d)
            //- necessary because d.url omits the file extension & so doesn't work on S3
            a(href=get_url(d),style="font-weight:bold;")
              | #{d.title} (#{d.year})
            br
            | --- by #{d.authors}#{d.journal ? ', ' : ''}
            if d.journal
              u #{d.journal}
            if d.journal && d.volume
              | 
              | #{d.volume}
            if d.journal && d.number
              | (#{d.number})
            if d.booktitle
              | 
              | in 
              if d.editor 
                | #{d.editor} (eds.), 
              i #{d.booktitle} 
              if !d.journal && d.volume
                |  Volume #{d.volume}
              if !d.journal && d.number
                |  , Number #{d.number}
            if d.address
              | , #{d.address}
            if d.address && d.publisher
              | : 
            | #{d.publisher}
            if d.pages
              | , pp. #{d.pages}
            
            if d.isbn
              | ; ISBN: #{d.isbn}
            if d.issn
              | ; ISSN: #{d.issn}
 
            br

            if d.pdf || d.doi || d.link
              | --- links: 
              if d.pdf
                a(href='/pdf/#{d.basename}.pdf',target='_blank') [pdf] 
              if d.doi
                | external 
                a(href='http://dx.doi.org/#{d.doi}',target='_blank') [doi: #{d.doi}] 
              if d.link
                a(href=d.link,target='_blank') [publisher's page]
        
          h3 Book

          p
            a(href='/writing/developing_mind.html',style="font-weight:bold;") The Developing Mind: A Philosophical Introduction (2020)
            br
            | --- London: Routledge 
            a(href='https://www.routledge.com/The-Developing-Mind-A-Philosophical-Introduction/Butterfill/p/book/9780415566230',target='_blank') [publisher's page]
            br
            | --- links: 
            a(href='/pdf/developing_mind_contents.pdf') contents [pdf]
            | ; 
            a(href='/pdf/developing_mind_introduction.pdf') introduction [pdf]

          h3 Forthcoming
          each d in getCollection("documents").findAll({isPublication:true, isForthcoming:true},[{year:-1,sort_order:1}]).toJSON()
            p
              mixin display_publication(d)

          h3 Published
  
          each d in getCollection("documents").findAll({isPublication:true, isForthcoming:false},[{year:-1,sort_order:1}]).toJSON()
            p
