<file path="src/layouts/post.html.eco">
---
layout: default
---

<article id="post" class="post">
	<h1><%= @document.title %></h1>
	<div class="post-content"><%- @content %></div>
</article>

<footer>
	<% if  @document.relatedDocuments and @document.relatedDocuments.length: %>
	<section id="related">
		<h3>Related Posts</h3>
		<nav class="linklist">
			<% for document in @document.relatedDocuments: %>
				<li><span><%= document.date.toShortDateString() %></span>
				&raquo;
				<a href="<%= document.url %>"><%= document.title %></a></li>
			<% end %>
		</nav>
	</section>
	<% end %>
</footer>
</file>

<file path="src/layouts/slides.html.jade">
---
layout: 'default'
---

- var scripts = getBlock('scripts')

h3
  a(href="/") Home
  | 
  | >> Teaching >> 
  //- fragile and stupid: assume the following will get us a link to the page for the lectures!
  a(href="/teaching/"+document.base_dir+'.html') #{document.lecture_series_name}
  |  
  | >> #{document.basename}


  .deck-container
    each f in getFilesAtPath('img/teaching/'+document.base_dir+'/'+document.basename+'/',[{basename:1}]).toJSON()
      section.slide
        img(src=f.url)
  
    //-- deck.navigation snippet 
    a(href="#",class="deck-prev-link",title="Previous") &#8592
    a(href="#",class="deck-next-link",title="Next") &#8594
  
    //-- deck.status snippet 
    p.deck-status
      span.deck-status-current
      | / 
      span.deck-status-total
    
    //-- deck.automatic snippet
    .deck-automatic-link(title="Play/Pause") Play/Pause

!= content
</file>

<file path="src/layouts/teaching.html.jade">
---
layout: 'default'
---

- var scripts = getBlock('scripts')

h3
  a(href="/") Home
  | 
  | >> 
  a(href="/#teaching") Teaching
  |  
  | >>


h1 #{document.title} (#{document.term} #{document.year})

p by #{document.authors}

- d = document

  if document.abstract
    .row
      .span12
        h3 Description
        p
          != document.abstract

  .row    
    .span12
      h3 Course Outline
      p Download: 
        a(href='/pdf/teaching/#{d.basename}/course_outline.pdf',target='_blank') course outline [pdf]

  each title,prefix in document.lectures
    .row
      .span12
        - files = getFilesAtPath('img/teaching/'+document.basename+'/lecture'+prefix+'/',[{basename:1}]).toJSON()
        a(href='/'+document.relativeBase+'/lecture'+prefix+'.html')
          img(src=files[0].url,width="300",style="float:left;margin-right:3em;margin-bottom:3em;")
        h3 Lecture #{prefix} : #{title}
        p 
          a(href='/pdf/teaching/#{d.basename}/lecture#{prefix}.handout.pdf',target='_blank') handout [pdf], 
          a(href='/pdf/teaching/#{d.basename}/lecture#{prefix}.slides.pdf',target='_blank') slides [pdf]

!= content
</file>

